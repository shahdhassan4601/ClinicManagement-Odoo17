<odoo>
    <data>
        <record id="patient_sequence" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">patient.sequence</field>
            <field name="prefix">P</field>
            <field name="padding">6</field>
        </record>

        <record id="appointment_sequence" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">appointment.sequence</field>
            <field name="prefix">AP</field>
            <field name="padding">8</field>
        </record>

        <record id="invoice_sequence" model="ir.sequence">
            <field name="name">Invoices Sequence</field>
            <field name="code">invoice.sequence</field>
            <field name="prefix">IN</field>
            <field name="padding">6</field>
        </record>
        
        <record id="prescription_sequence" model="ir.sequence">
            <field name="name">Prescription Sequence</field>
            <field name="code">prescription.sequence</field>
            <field name="prefix">PR</field>
            <field name="padding">6</field>
        </record>

    </data>
</odoo>